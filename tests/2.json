{
    "apiVersion": "security.kubearmor.com/v1",
    "kind": "KubeArmorPolicy",
    "metadata": {
        "name": "autopol-system-4069661765",
        "namespace": "wordpress-mysql"
    },
    "spec": {
        "severity": 1,
        "selector": {
            "matchLabels": {
                "app": "wordpress"
            }
        },
        "process": {
            "matchPaths": [
                {
                    "path": "/bin/mkdir",
                    "fromSource": [
                        {
                            "path": "/bin/bash"
                        }
                    ]
                },
                {
                    "path": "/bin/rm",
                    "fromSource": [
                        {
                            "path": "/bin/bash"
                        }
                    ]
                },
                {
                    "path": "/bin/sed",
                    "fromSource": [
                        {
                            "path": "/bin/bash"
                        }
                    ]
                },
                {
                    "path": "/usr/local/bin/apache2-foreground"
                },
                {
                    "path": "/usr/local/bin/php",
                    "fromSource": [
                        {
                            "path": "/bin/bash"
                        }
                    ]
                },
                {
                    "path": "/usr/sbin/apache2"
                },
                {
                    "path": "/usr/bin/head"
                },
                {
                    "path": "/usr/bin/cut"
                },
                {
                    "path": "/usr/bin/dirname"
                },
                {
                    "path": "/usr/bin/sha1sum"
                }
            ],
            "matchDirectories": [
                {
                    "dir": "/usr/bin/",
                    "recursive": true,
                    "fromSource": [
                        {
                            "path": "/bin/bash"
                        }
                    ]
                }
            ]
        },
        "file": {
            "matchPaths": [
                {
                    "path": "/dev/urandom"
                },
                {
                    "path": "/etc/apache2/envvars"
                },
                {
                    "path": "/etc/ld.so.cache"
                },
                {
                    "path": "/etc/passwd"
                },
                {
                    "path": "/run/lock/apache2/mpm-accept.1"
                },
                {
                    "path": "/run/lock/apache2/rewrite-map.1"
                },
                {
                    "path": "/usr/local/etc/php/conf.d",
                    "fromSource": [
                        {
                            "path": "/usr/local/bin/php"
                        }
                    ]
                },
                {
                    "path": "/usr/local/lib/php/extensions/no-debug-non-zts-20131226/gd.so",
                    "fromSource": [
                        {
                            "path": "/usr/local/bin/php"
                        }
                    ]
                },
                {
                    "path": "/usr/local/lib/php/extensions/no-debug-non-zts-20131226/mysqli.so",
                    "fromSource": [
                        {
                            "path": "/usr/local/bin/php"
                        }
                    ]
                },
                {
                    "path": "/usr/local/lib/php/extensions/no-debug-non-zts-20131226/opcache.so"
                },
                {
                    "path": "/var/log/apache2/access.log"
                },
                {
                    "path": "/var/log/apache2/error.log"
                },
                {
                    "path": "/var/log/apache2/other_vhosts_access.log"
                }
            ],
            "matchDirectories": [
                {
                    "dir": "/etc/",
                    "recursive": true
                },
                {
                    "dir": "/proc/",
                    "recursive": true
                },
                {
                    "dir": "/tmp/",
                    "recursive": true,
                    "fromSource": [
                        {
                            "path": "/usr/local/bin/php"
                        }
                    ]
                },
                {
                    "dir": "/usr/lib/apache2/modules/",
                    "recursive": true
                },
                {
                    "dir": "/usr/lib/x86_64-linux-gnu/",
                    "recursive": true
                },
                {
                    "dir": "/usr/local/etc/php/conf.d/",
                    "recursive": true,
                    "fromSource": [
                        {
                            "path": "/usr/local/bin/php"
                        }
                    ]
                },
                {
                    "dir": "/var/www/html/",
                    "recursive": true,
                    "fromSource": [
                        {
                            "path": "/bin/sed"
                        }
                    ]
                },
                {
                    "dir": "/lib/x86_64-linux-gnu/",
                    "recursive": true
                }
            ]
        },
        "action": "Allow"
    }
}
